[INPUT]
o1_y:4...10
o1_x:4...4
o2_y:1...7
o2_x:6...6

a_x: 0...11
a_y: 0...11

time:0...31

[OUTPUT]
controller:0...2
direction:0...4 
#0 = no motion, 1 = North, 2 = East, 3 = South, 4 = West

[SYS_INIT]

direction = 0

[ENV_INIT]
time = 0
o1_y = 7
o2_y = 4
a_x = 2
a_y = 2


[SYS_TRANS]

a_x' != 0
a_y' != 0
a_x' != 11 
a_y' != 11
time <= 25


!(a_x <= o1_x & a_x' >= o1_x & (a_y = o1_y | a_y' = o1_y | a_y = o1_y'))
!(a_x >= o1_x & a_x' <= o1_x & (a_y = o1_y | a_y' = o1_y | a_y = o1_y'))
!(a_x <= o2_x & a_x' >= o2_x & (a_y = o2_y | a_y' = o2_y | a_y = o2_y'))
!(a_x >= o2_x & a_x' <= o2_x & (a_y = o2_y | a_y' = o2_y | a_y = o2_y'))


[SYS_LIVENESS]
a_x' = 9 & a_y' = 9
a_x' = 2 & a_y' = 2
#time = 40

[ENV_TRANS]

!(a_x = 2 & a_y = 2) -> time' = time + 1 
(a_x = 2 & a_y = 2) ->  time' = 0

#time' = time + 1 
o1_x' = o1_x

o1_y'+ 1 >= o1_y
o1_y+ 1 >= o1_y'

o2_y'+ 1 >= o2_y
o2_y+ 1 >= o2_y'

(direction = 0) -> ((a_x' = a_x) & (a_y' = a_y))



